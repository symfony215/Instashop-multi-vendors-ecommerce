{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "astrotomic/laravel-translatable": "^11.8",
        "calebporzio/parental": "^0.10",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^1.0",
        "laravel/framework": "^7.0",
        "laravel/passport": "^7.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "munafio/chatify": "^1.0",
        "nwidart/laravel-modules": "^7.0",
        "spatie/laravel-medialibrary": "^7.6",
        "wikimedia/composer-merge-plugin": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.15",
        "fzaninotto/faker": "^1.9.1",
        "martinlindhe/laravel-vue-i18n-generator": "^0.1.42",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "Modules/*/composer.json"
            ],
            "autoload": true,
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "phs-fresh" : "php artisan migrate:fresh --seed",
        "php-cs:issues": "vendor/bin/php-cs-fixer fix --diff --dry-run",
        "php-cs:fix": "vendor/bin/php-cs-fixer fix",
        "app:clear": "php artisan clear-compiled && php artisan cache:clear && php artisan config:clear && php artisan debugbar:clear && php artisan route:clear && php artisan view:clear",
        "auto-complete:generate": [
            "@php artisan ide-helper:meta --ansi --quiet",
            "@php artisan ide-helper:generate --ansi --quiet",
            "@php artisan ide-helper:models --nowrite --quiet"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate --ansi --quiet",
            "@php artisan ide-helper:meta --ansi --quiet"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
